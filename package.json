    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.3.0",
    "eslint": "^8.57.0",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-vue": "^9.17.0",
    "postcss": "^8.4.34",
    "postcss-import": "^14.1.0",
    "postcss-nesting": "^12.0.2",
    "prettier": "^3.0.3",
    "storybook": "^8.0.5",
    "typescript": "~5.3.0",
    "vite": "^5.0.12",
    "vue": "^3.4.0",
    "vue-loader": "^17.4.2",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.27"
