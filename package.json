{
  "name": "vuiii",
  "version": "1.0.0-beta.85",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/greegus/vuiii.git"
  },
  "files": [
    "dist",
    "CLAUDE.md"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/vuiii.js"
    },
    "./style.css": "./dist/vuiii.css"
  },
  "scripts": {
    "dev": "npm run storybook",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "oxlint --fix",
    "format": "oxfmt",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic --exit-zero-on-changes",
    "docs": "npx jiti scripts/generate-docs.ts",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:generate": "npx jiti scripts/generate-vitepress-docs.ts",
    "deploy": "npx jiti scripts/deploy.ts"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.1",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-links": "^10.2.8",
    "@storybook/vue3-vite": "^10.2.8",
    "@tsconfig/node24": "^24.0.3",
    "@types/node": "^24.10.4",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/tsconfig": "^0.8.1",
    "jiti": "^2.6.1",
    "npm-run-all2": "^8.0.4",
    "oxfmt": "^0.24.0",
    "oxlint": "^1.1.0",
    "storybook": "^10.2.8",
    "typescript": "~5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitepress": "^1.6.4",
    "vue-loader": "^17.4.2",
    "vue-router": "^4.2.5",
    "vue-tsc": "^3.2.2"
  },
  "peerDependencies": {
    "vue": "^3.5.26",
    "vue-router": "5.x"
  },
  "engines": {
    "node": ">=24.0.0"
  }
}
